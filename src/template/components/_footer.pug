//begin footer
+b.FOOTER.footer
	+e.category
		+e.container.container
			+e.category-inner

				-
					categoryList = [{
					title: 'О нас',
					listText: ['Подкатегоря 1', 'Подкатегоря 1', 'Подкатегоря 1', 'Подкатегоря 1']
					}, {
					title: 'Курсы',
					listText: ['Подкатегоря 1', 'Подкатегоря 1', 'Подкатегоря 1']
					}, {
					title: 'Методика',
					listText: ['Подкатегоря 1', 'Подкатегоря 1', 'Подкатегоря 1', 'Подкатегоря 1']
					}, {
					title: 'Языки',
					listText: ['Немецкий', 'Испанский', 'Английский', 'Китайский']
					}, {
					title: 'Группы',
					listText: ['Подкатегоря 1', 'Подкатегоря 1', 'Подкатегоря 1']
					}, {
					title: 'Прайс',
					listText: ['Подкатегоря 1', 'Подкатегоря 1', 'Подкатегоря 1', 'Подкатегоря 1']
					}]

				mixin categoryItem(item)
					+e.category-list.circle-list
						+e.category-list-title
							a(href='#!')= item.title
						ul
							each text in item.listText
								li
									a.link-decor(href='#!')= text
				for item in categoryList
					+categoryItem(item)

	+e.line
		+e.line-container.container
			+e.logo
				+e.A.logo-link(href='#!')
					+e.IMG(src='static/img/other/logo-sm_white.png' alt='IBIS')
				+e.logo-text
					p Copyright © 2015 Lingua-Seasons.
					p All rights reserved

			+e.contacts
				+e.contacts-block
					p Следуйте за нами:
					-socialsClass = 'footer__socials'
					include ../components/socials
				+e.contacts-block
					+e.contacts-block-text
						svg(class='icon-tel' xmlns='http://www.w3.org/2000/svg', xmlns:xlink='http://www.w3.org/1999/xlink', width='17', height='16', viewbox='0 0 17 16')
